   <string notr="true">/* 极淡蓝色整体风格（不改布局，只做视觉微调） */
QMainWindow,
QWidget#centralwidget,
QWidget#scrollAreaWidgetContents {
    background-color: #E6F1FF;
}

QScrollArea {
    background: transparent;
    border: none;
}

QGroupBox {
    background-color: rgba(255, 255, 255, 235);
    border: 1px solid #BFD8FF;
    border-radius: 6px;
    margin-top: 10px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 10px;
    padding: 0 6px;
    color: #2B4A66;
}

QLabel {
    color: #1F2D3D;
}

QLineEdit,
QPlainTextEdit,
QTextEdit,
QSpinBox,
QDoubleSpinBox,
QComboBox {
    background-color: #FFFFFF;
    border: 3px solid #5B9FFF;
    border-radius: 4px;
    padding: 4px 6px;
}

QLineEdit:focus,
QPlainTextEdit:focus,
QTextEdit:focus,
QSpinBox:focus,
QDoubleSpinBox:focus,
QComboBox:focus {
    border: 3px solid #4A8EF5;
}

/* 只读/不可编辑：用偏灰蓝底 + 更弱文字/边框，和可输入控件明显区分 */
QLineEdit[readOnly=&quot;true&quot;],
QPlainTextEdit[readOnly=&quot;true&quot;],
QTextEdit[readOnly=&quot;true&quot;] {
    background-color: #F2F7FF;
    color: #5E6F80;
    border: 1px dashed #AFCBFF;
}

QLineEdit[readOnly=&quot;true&quot;]:hover,
QPlainTextEdit[readOnly=&quot;true&quot;]:hover,
QTextEdit[readOnly=&quot;true&quot;]:hover,
QLineEdit[readOnly=&quot;true&quot;]:focus,
QPlainTextEdit[readOnly=&quot;true&quot;]:focus,
QTextEdit[readOnly=&quot;true&quot;]:focus {
    background-color: #F2F7FF;
    color: #5E6F80;
    border: 1px dashed #AFCBFF;
}

/* 完全不可用（disabled）：进一步变淡，提示“当前不可操作/不可输入” */
QLineEdit:disabled,
QPlainTextEdit:disabled,
QTextEdit:disabled,
QSpinBox:disabled,
QDoubleSpinBox:disabled,
QComboBox:disabled {
    background-color: #F3F6FA;
    color: #7B8DA0;
    border: 1px solid #D7E3F2;
}

QPushButton,
QToolButton {
    color: #0B2A44;
    font-weight: 600;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #EAF5FF,
                                stop:1 #B9DCFF);
    border: 1px solid #4F95FF;
    border-bottom: 2px solid #3F8CFF;
    border-radius: 6px;
    padding: 7px 12px;
    min-height: 28px;
}

QPushButton:hover,
QToolButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #F3FAFF,
                                stop:1 #C7E3FF);
    border-color: #3F8CFF;
    border-bottom-color: #2F7FFF;
}

QPushButton:pressed,
QToolButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #A8D0FF,
                                stop:1 #C0E0FF);
    border-color: #2F7FFF;
    border-bottom-color: #2F7FFF;
}

QPushButton:focus,
QToolButton:focus {
    border-color: #3F8CFF;
}

QPushButton:disabled,
QToolButton:disabled {
    color: #7B8DA0;
    background-color: #F4F7FB;
    border-color: #D7E3F2;
}

QScrollBar:vertical {
    background: transparent;
    width: 10px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background: #BFD8FF;
    min-height: 20px;
    border-radius: 5px;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar:horizontal {
    background: transparent;
    height: 10px;
    margin: 0px;
}

QScrollBar::handle:horizontal {
    background: #BFD8FF;
    min-width: 20px;
    border-radius: 5px;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    width: 0px;
}</string>
